N8N_HOST=n8n.example.ts.net
N8N_PORT=5678
N8N_PROTOCOL=https
NODE_ENV=production
WEBHOOK_URL=https://n8n.example.ts.net/
GENERIC_TIMEZONE=UTC
N8N_HIDE_USAGE_PAGE=false
N8N_PROXY_HOPS=1
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336

# Database (Postgres)
# These credentials are passed to both the Postgres container and n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=n8n

# Tailscale Configuration
TS_AUTHKEY=tskey-auth-example
# TS_HOSTNAME=n8n